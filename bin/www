const app = require("../app");
require("dotenv").config();

const port = process.env.PORT || 3000;

app.listen(port, () => {
    console.log(`Listen on port : http://localhost:${port}`);
    console.log(
        "Documentation : https://documenter.getpostman.com/view/23034311/2s83zdukYg"
    );
});
